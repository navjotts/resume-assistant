html
    head
        title='Training'
        link(rel='stylesheet', href='https://stackpath.bootstrapcdn.com/bootstrap/4.1.0/css/bootstrap.min.css')
        link(rel='stylesheet', href='/stylesheets/style.css')
        script(src='https://code.jquery.com/jquery-3.1.1.min.js')
        script(src='/js/Client.js')
    body
        div(class='container-fluid')
            div(class='center')
                div(class='title')
                    p='Training'
            div(id='training-header', class='training-header')
                a(class='left-link', href='#', onclick='fetchResumes()')='Resumes'
                a(class='right-link', href='#', onclick='fetchJobs()')='Jobs'
            div(class='training-list')
                div(id='resumes-list', class='training-list-left')
                div(id='jobs-list', class='training-list-right')